{
  "schemaVersion": 1,
  "id": "modid",
  "version": "${version}",

  "name": "Vanilla Extended",
  "description": "A minecraft mod that focuses on 1.16+ game mechanics and items, adding onto and improving vanilla minecraft mechanics/items.",
  "authors": [
    "SirLynx_, contributors"
  ],
  "contact": {
    "homepage": "https://sirlynx.github.io/vanillaextended",
    "sources": "https://github.com/SirLynx/VanillaExtended"
  },

  "license": "Apache-2.0",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.sirlynx.VanillaExtended"
    ]
  },
  "mixins": [
    "modid.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": "1.17.x",
    "java": ">=16"
  },
  "suggests": {
    "another-mod": "*"
  }
}
